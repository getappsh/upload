# Flag if we want to use the delviery servcie as a cache service ---- is need also the aws.env
USE_CACHE=# "true / false"

CACHE_CLEAR_TIME_EXPRESSION=# 'Specifies the cron schedule for clearing the cache (e.g., 0 0 * * * for midnight daily).'
CACHE_EXPIRATION_HOURS=# "Sets the duration (in hours) for how long cached items remain valid before expiration (default is 12)"
CACHE_SYNC_TIME_EXPRESSION=# 'Specifies the cron schedule for syncing the cache data between the DB an S3 (e.g., 0 0 * * * for midnight daily).'