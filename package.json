{
  "name": "getapp",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build:app": "nest build",
    "build:migration": "nest build --webpack --webpackPath webpack/typeorm-cli.config.js && nest build --webpack --webpackPath webpack/migrations.config.js",
    "build": "npm run build:migration && npm run build:app",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start:no-mig": "nest start",
    "start:dev:no-mig": "nest start --watch",
    "start": "npm run migration:run && nest start",
    "start:dev": "npm run migration:run && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/api/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:create": "typeorm-ts-node-esm migration:create  libs/common/src/database/migration/%npm_config_name%",
    "migration:show": "typeorm-ts-node-esm -d libs/common/src/database/config/ormconfig.ts migration:show",
    "migration:generate": "typeorm-ts-node-esm -d libs/common/src/database/config/ormconfig.ts migration:generate libs/common/src/database/migration/%npm_config_name%",
    "migration:run": "typeorm-ts-node-esm -d libs/common/src/database/config/ormconfig.ts migration:run",
    "migration:revert": "typeorm-ts-node-esm -d libs/common/src/database/config/ormconfig.ts migration:revert",
    "all:dev": "concurrently  --names \"API,UPLOAD,DISCOVERY,OFFERING,DELIVERY,PROJECT-MANAGEMENT,DEPLOY,GET-MAP\" \"npm -C ../api run start:dev:no-mig api\" \"npm -C ../upload run start:dev:no-mig upload\"  \"npm -C ../discovery run start:dev:no-mig discovery\"  \"npm -C ../offering run start:dev:no-mig offering\"  \"npm -C ../delivery run start:dev:no-mig delivery\"  \"npm -C ../project-management run start:dev:no-mig project-management\"  \"npm -C ../deploy run start:dev:no-mig deploy\" \"npm -C ../get-map run start:dev:no-mig get-map\"",
    "build:all": "concurrently  --names \"API,UPLOAD,DISCOVERY,OFFERING,DELIVERY,PROJECT-MANAGEMENT,DEPLOY,GET-MAP\" \"npm run build\" \"npm run build upload\"  \"npm run build discovery\"  \"npm run build offering\"  \"npm run build delivery\"  \"npm run build project-management\"  \"npm run build deploy\"  \"npm run build get-map\""
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.296.0",
    "@aws-sdk/lib-storage": "^3.412.0",
    "@aws-sdk/s3-request-presigner": "^3.296.0",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.3.9",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.9",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/microservices": "^9.3.9",
    "@nestjs/platform-express": "^9.3.9",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/typeorm": "^10.0.0",
    "@types/dockerode": "^3.3.16",
    "axios": "^1.3.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "concurrently": "^8.0.1",
    "dockerode": "^3.3.5",
    "dotenv": "^16.0.3",
    "geojson-validation": "^1.0.2",
    "kafkajs": "git://github.com/getappsh/kafkajs/tree/bug#17f3c7f0c1b9509e4071ad6a5a77e922362d19c0",
    "nanoid": "^3.3.7",
    "nest-keycloak-connect": "^1.9.1",
    "nestjs-cls": "^4.2.0",
    "nestjs-elastic-apm": "^1.3.0",
    "nestjs-pino": "^4.0.0",
    "pg": "^8.9.0",
    "pino-http": "^9.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.9",
    "@types/express": "^4.17.17",
    "@types/jest": "29.2.4",
    "@types/multer": "^1.4.7",
    "@types/node": "18.11.18",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.3.1",
    "prettier": "^2.8.4",
    "shebang-loader": "^0.0.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4",
    "webpack-ignore-dynamic-require": "^1.0.0",
    "webpack-merge": "^5.10.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@app/root(|/.*)$": "<rootDir>/apps/$1"
    }
  }
}
